<?php

/**
 * @file
 * Install, update and uninstall functions for the File Log module.
 */

/**
 * Implements hook_uninstall().
 */
function file_log_uninstall() {
  variable_del('file_log_path');
  variable_del('file_log_filename');
  variable_del('file_log_json');
  variable_del('file_log_format');
}
